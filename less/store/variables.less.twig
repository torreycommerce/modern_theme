// {{ '2' }}
// Colors 
// ----------------------------------------------------------------------------------------------------

// Brand
// -------------------------
@brand-primary:									#{{ settings.current["colors"]["brand"]["brand-primary"]["value"] }};
@brand-secondary:								#{{ settings.current["colors"]["brand"]["brand-secondary"]["value"] }};
@brand-success:         						#{{ settings.current["colors"]["brand"]["brand-success"]["value"] }};
@brand-info:         		   					#{{ settings.current["colors"]["brand"]["brand-info"]["value"] }};
@brand-warning:     						    #{{ settings.current["colors"]["brand"]["brand-warning"]["value"] }};
@brand-danger:     							    #{{ settings.current["colors"]["brand"]["brand-danger"]["value"] }};

// Gray
// -------------------------
@gray-darker:									lighten(#000, 13.5%); // #222
@gray-dark:										lighten(#000, 20%);   // #333
@gray:											lighten(#000, 33.5%); // #555
@gray-light:									lighten(#000, 60%);   // #999
@gray-lighter:									lighten(#000, 93.5%); // #eee

// Feedback states
// -------------------------
@state-success-text:							#EEEEEE;
@state-success-bg:								@brand-success;
@state-success-border:							darken(spin(@state-success-bg, -10), 5%);

@state-info-text:								#FFFFFF;
@state-info-bg:									@brand-info;
@state-info-border:								darken(spin(@state-info-bg, -10), 7%);

@state-warning-text:							#FFFFFF;
@state-warning-bg:								@brand-warning;
@state-warning-border:							darken(spin(@state-warning-bg, -10), 5%);

@state-danger-text:								#FFFFFF;
@state-danger-bg:								@brand-danger;
@state-danger-border:							darken(spin(@state-danger-bg, -10), 5%);

@border-color:									@gray-lighter;



//
// Scaffolding
// ----------------------------------------------------------------------------------------------------

// Background color for `<body>`
// -------------------------
@body-bg:					                    #{{ settings.current["colors"]["background"]["body-bg"]["value"] }};
@shop-bg-image:									0 none;
@shop-bg-image-retina:							0 none;

// Text color on `<body>`
// -------------------------
@text-color:									#{{ settings.current["colors"]["content"]["text-color"]["value"] }};

// Link color
// -------------------------
@link-color:    		                        #{{ settings.current["colors"]["content"]["link-color"]["value"] }};
@link-hover-color:      						#{{ settings.current["colors"]["content"]["link-hover-color"]["value"] }};

// Price color
// -------------------------
@price-color:                         			#b2246b;

// Hr
// -------------------------
@hr-border:										#333333;



//
// Typography
// ----------------------------------------------------------------------------------------------------

// Body text
// -------------------------
@font-family-sans-serif:						{{ settings.current["typography"]["base_styles"]["base-typeface"]["value"] }};
@font-family-serif:								@font-family-sans-serif;
@font-family-monospace:   						@font-family-sans-serif;
@font-family-lucida:							{{ settings.current["typography"]["h1_heading_styles"]["heading-typeface"]["value"] }};
@line-height-base:								1.428571429; // 20/14

@font-size-base:								{{ settings.current["typography"]["base_styles"]["font-size-base"]["value"] }};
@font-size-large:         						ceil((@font-size-base * 1.25)); // ~18px
@font-size-small:         						ceil((@font-size-base * 0.85)); // ~12px

@font-size-h1:									{{ settings.current["typography"]["h1_heading_styles"]["font-size-h1"]["value"] }};
@font-size-h2:									{{ settings.current["typography"]["h2_heading_styles"]["font-size-h2"]["value"] }};
@font-size-h3:									{{ settings.current["typography"]["h3_heading_styles"]["font-size-h3"]["value"] }};
@font-size-h4:									ceil((@font-size-base * 1.25));	// ~18px
@font-size-h5:									@font-size-base;				// ~15px
@font-size-h6:									ceil((@font-size-base * 0.90));	// ~13px

// Unit-less `line-height` for use in components like buttons.
@line-height-base:								1.428571429; // 20/14

// Computed "line-height" (`font-size` * `line-height`) for use with `margin`, `padding`, etc.
@line-height-computed:    floor((@font-size-base * @line-height-base)); // ~20px

// Headings
// -------------------------
@headings-color:								#{{ settings.current["colors"]["headers"]["headings-color"]["value"] }};
@headings-font-family:							{{ settings.current["typography"]["h1_heading_styles"]["heading-typeface"]["value"] }};
@headings-font-weight:							{{ settings.current["typography"]["h1_heading_styles"]["headings-font-weight"]["value"] }};
@headings-line-height:							1.1;
@headings-small-color:							#{{ settings.current["colors"]["headers"]["headings-small-color"]["value"] }};

@page-header-color:								#{{ settings.current["colors"]["headers"]["page-header-color"]["value"] }};
@page-header-border-color:						#{{ settings.current["colors"]["headers"]["page-header-border-color"]["value"] }};



