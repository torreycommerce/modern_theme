{
    "EXTEND": [
        "json store.json"
    ],
    "components": {
        "dbsite":{
           "enableProfiling":true,
           "enableParamLogging":true
        },
        "nvp": {
            "class":"ext.Paypal.PaypalNVP",
            "user":"",
            "pwd":"",
            "signature":"",
            "sandbox":true
        },
        "payflowpro":{
            "class":"ext.Paypal.PaypalPayflowPro",
            "user":"",
            "vendor":"",
            "partner":"PayPal",
            "pwd":"",
            "sandbox":false
        },
        "imageManager":{
          "storage":{
            "url":"http:\/\/localhost:8888\/acenda\/bones\/store\/images\/bucket"
          }
        }
    },
    "params": {
        "url":{
            "storeImage":"\/images\/",
            "live": "",
            "preview": ""
        },
      "theme" : {
        "current" : "theme1"
      },
      "order": {
        "cancellation_time": "30minutes"
      },
      "social_settings": {
        "facebook": {
          "_comment":" 'like_type' can be set to like or recommend",
          "active_like":false,
          "like_type":"like",
          "active_share":false,
          "show_count":false,
          "active_metatags":false,
          "appId":"",
          "facebook_page":"",
          "active_follow_page_button":false
        },
        "twitter": {
          "active_tweet":false,
          "add_acct_to_tweet":false,
          "twitter_account":"",
          "active_metatags":false,
          "twitter_page":"",
          "appId":"",
          "active_follow_page_button":false
        },
        "pinterest": {
          "_comment": "Shape Can Be Set To 'rectangular' Or 'circular'",
          "active_pin_it":false,
          "show_count":false,
          "full_page_name":"",
          "pinterest_page":"",
          "appId":"",
          "active_follow_page_button":false
        },
        "google": {
          "active_plus_one":false,
          "full_page_name":"",
          "google_page":"",
          "appId":"",
          "active_follow_page_button":false
        }
      },
      "live_chat": {
        "active_chat":false,
        "chat_licence":""
      },
      "seo": {
        "img":"",
        "description":"",
        "title":"Home"
      },
      "site": {
        "title":"",
        "url":""
      },
      "adminMenu": {
          "sideNav" : [
              { "title":"Customers","icon":"fa-user", "uri": "/customer"},
              { "title":"Products","icon":"fa-barcode", "uri": "/product"},
              { "title":"Categories","icon":"fa-list", "uri": "/category"},
              { "title":"Orders","icon":"fa-inbox", "uri": "/order"},
              { "title":"Coupons","icon":"fa-ticket", "uri": "/coupon"},
              { "title":"Shipping Methods","icon":"fa-truck", "uri": "/shippingmethod"},
              { "title":"Address Blacklist","icon":"fa-list", "uri": "/addressblacklist"},
              { "title":"IP Blacklist","icon":"fa-user", "uri": "ipblacklist"}
          ],
          "userNav": [
          ]
      },
      "returnTime":"-30 days",
      "feed": {
          "google":{
              "site_id":"",
              "filename": ""
          }
      },
      "tax": {
          "states": ["CA"]
      },
      "contact-us":{
          "to":"no-reply@acenda.com",
          "from":"no-reply@acenda.com"
      },
      "notifications":{
          "from":"no-reply@acenda.com",
          "order":{
              "placed":{
                  "template":"email/order-placed.html.twig",
                  "subject":"Order {{ order_number }} placed"
              },
              "shipped":{
                  "template":"email/order-shipped.html.twig",
                  "subject":"Order {{ order_number }} shipped"
              },
              "cancelled":{
                  "template":"email/order-cancelled.html.twig",
                  "subject":"Order {{ order_number }} cancelled"
              },
              "return":{
                  "template":"email/order-return.html.twig",
                  "subject":"Order {{ order_number }} return"
              },
              "returnupdate":{
                  "template":"email/order-return-update.html.twig",
                  "subject":"Order return update"
              },
              "shipping-update":{
                  "template":"email/order-shipping-update.html.twig",
                  "subject":"Order shipping update"
              }
          },
          "customer-reset-password":{
              "template":"email/customer-reset-password.html.twig",
              "subject":"Password reset request"
          },
          "customer-new-account":{
            "template":"email/new-account.html.twig",
            "subject":"Welcome to {{ site_name }}"
          },
          "customer-new-subscriber":{
                "template":"email/new-subscriber.html.twig",
                "subject":"You subscribed to {{ site_name }}"
          },
          "administrator-invitation":{
              "template":"email/administrator-invitation.html.twig",
              "subject":"You have been invited to the store %store%"
          },
          "administrator-forgot-password":{
              "template":"email/administrator-forgot-password.html.twig",
              "subject":"You have lost your password? No worries!"
          }
      },
      "business-info":{
        "business-name":"",
        "business-phone":"",
        "business-city":"",
        "business-zip":"",
        "business-state":""
      }
  }
}
